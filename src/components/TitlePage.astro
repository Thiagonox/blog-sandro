---
import Title from './Title.astro'
import Shape from './icons/Shape.astro'
type Props = {
	title: string
}

const { title } = Astro.props
---

<div class='flex justify-start items-center gap-2 title'>
	<img
		src='https://assets.tina.io/774ee9f6-4695-432e-b755-e33ee3ae116a/logo-sandro.png'
		alt='Shape'
		class='w-36 h-36 md:w-52 md:h-52'
	/>
	<Title>{title}</Title>
</div>
